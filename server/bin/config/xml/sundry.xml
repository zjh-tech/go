<?xml version='1.0' encoding='utf-8'?>

<config>
    <!-- 日程疲劳计算 -->
    <strength1>1.5</strength1>
    <strength2>1</strength2>
    <strength3>0.5</strength3>
    
    <strength>
        <random start="0" end="0" ratio1="70" ratio2="100" ratio3="100"   />
        <random start="1" end="10" ratio1="50" ratio2="90" ratio3="100"   />
        <random start="11" end="30" ratio1="40" ratio2="80" ratio3="100"  />
        <random start="31" end="50" ratio1="20" ratio2="60" ratio3="100"  />
        <random start="51" end="70" ratio1="10" ratio2="50" ratio3="100"  />
        <random start="71" end="100" ratio1="0" ratio2="30" ratio3="100"  />
    </strength>

    <!-- 日程压力计算 -->
    <pressurestart>10</pressurestart>
    <pressuredelta>5</pressuredelta>
    <pressure>
        <random start="0" end="25"   ratio="1"    />  
        <random start="26" end="50"  ratio="0.75" />
        <random start="51" end="75"  ratio="0.5"  />
        <random start="76" end="100" ratio="0.25" />
    </pressure>
	
    <!-- 幸运值-->
    <lucky>
        <random start="0" end="33"  level="3"   />
        <random start="34" end="66"  level="2"  />
        <random start="67" end="100"  level="1" />
    </lucky>

</config>

